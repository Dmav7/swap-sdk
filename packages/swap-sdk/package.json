{
  "name": "@vvs-finance/swap-sdk",
  "version": "0.0.3",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "typechain": "typechain --out-dir src/types/contracts --target ethers-v6 'src/abis/*.json'",
    "dev:build": "tsup --watch",
    "dev:tsc": "tsc -b --watch",
    "fix": "eslint src --fix",
    "lint": "eslint src",
    "ts-check": "tsc --noEmit",
    "test": "jest",
    "build": "rm -rf dist && tsup",
    "release-local": "yalc publish",
    "release": "pnpm publish"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "sourcemap": true,
    "clean": true,
    "dts": true
  },
  "exports": {
    "import": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "nock": "14.0.0-beta.19",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "bi-fraction": "^1.2.1",
    "ethers": "^6.13.4",
    "typechain": "^8.3.2"
  },
  "engines": {
    "node": ">=18"
  }
}
